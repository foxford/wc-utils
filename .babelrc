{
  "plugins": ["@babel/plugin-proposal-object-rest-spread"],
  "env": {
    "es": {
      "plugins": [
        "babel-plugin-annotate-pure-calls"
      ]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-env",{
          "modules": false,
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ],
      "plugins": [
        "babel-plugin-annotate-pure-calls",
        "@babel/plugin-transform-modules-commonjs"
      ]
    },
    "rollup": {
      "presets": [
        ["@babel/preset-env",{
          "modules": false,
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ]
    }
  },
}
